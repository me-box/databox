
  buildImage $COMPONENT_NAME
  seedManifest $COMPONENT_NAME
  installStores
else
  log "${COMPONENT_NAME} already installed, rebuilding and reseeding"
  pullChanges $COMPONENT_NAME
  buildImage $COMPONENT_NAME
  seedManifest $COMPONENT_NAME
  installStores
fi
  buildImage $COMPONENT_NAME
  seedManifest $COMPONENT_NAME
  installStores
else
  log "${COMPONENT_NAME} already installed, rebuilding and reseeding"
  pullChanges $COMPONENT_NAME
  buildImage $COMPONENT_NAME
  seedManifest $COMPONENT_NAME
  installStores
fi